fTHg_South_lr1 <- loadReg(fTHg ~ model(1), data=fTHg_South,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L", load.units="kg",
station="Cache Creek at South")
setSweave("fTHg_South_lr1_plot2",5,5)
plot(fTHg_South_lr1,which=1,set.up=FALSE) #plot 1: Calibrated regression model
graphics.off()
setSweave("fTHg_South_lr1_plot1",5,5)
plot(fTHg_South_lr1,which=2,set.up=FALSE) #plot 2:Estimates of log load versus model residuals from AMLE regression
graphics.off()
setSweave("fTHg_South_lr1_plot3",5,5)
plot(fTHg_South_lr1,which=3,set.up=FALSE)# plot 3: Estimates of log load versus squared model residuals
graphics.off()
setSweave("fTHg_South_lr1_plot4",5,5)
plot(fTHg_South_lr1,which=4,set.up=FALSE) # plot 4: Standardized Serial Correlation
graphics.off()
setSweave("fTHg_South_lr1_plot5",5,5)
plot(fTHg_South_lr1,which=5,set.up=FALSE) # plot 5: Normal probability plot for the model residuals (Quantile plot)
graphics.off()
#############################################################################################
