station="Cache Creek at CombSpillWay")
setSweave("fMeHg_CombSpillWay_lr5_plot2",5,5)
plot(fMeHg_CombSpillWay_lr5,which=1,set.up=FALSE) #plot 1: Calibrated regression model
graphics.off()
setSweave("fMeHg_CombSpillWay_lr5_plot1",5,5)
plot(fMeHg_CombSpillWay_lr5,which=2,set.up=FALSE) #plot 2:Estimates of log load versus model residuals from AMLE regression
graphics.off()
setSweave("fMeHg_CombSpillWay_lr5_plot3",5,5)
plot(fMeHg_CombSpillWay_lr5,which=3,set.up=FALSE)# plot 3: Estimates of log load versus squared model residuals
graphics.off()
setSweave("fMeHg_CombSpillWay_lr5_plot4",5,5)
plot(fMeHg_CombSpillWay_lr5,which=4,set.up=FALSE) # plot 4: Standardized Serial Correlation
graphics.off()
setSweave("fMeHg_CombSpillWay_lr5_plot5",5,5)
plot(fMeHg_CombSpillWay_lr5,which=5,set.up=FALSE) # plot 5: Normal probability plot for the model residuals (Quantile plot)
graphics.off()
#############################################################################################
#predict concntrations
###############################################################################################
preds_conc_lr5 <- predConc(fMeHg_CombSpillWay_lr5, CombSpillWayQ, by= "day", allow.incomplete= TRUE )
preds_conc_lr5
exportCSV(preds_conc_lr5, file.name = "CombSpillWay_fMeHg_conc_lr5_2013-2019.cvs")
###############################################################################################
##We plot the estimated unite conc Vs the obsereved unite conc in xls
###############################################################################################
###############################################################################################
#load calculations using selected model
###############################################################################################
preds_Flux_lr5day<-predLoad(fMeHg_CombSpillWay_lr5,CombSpillWayQ,load.units="kg",by="day",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
preds_Flux_lr5day
exportCSV(preds_Flux_lr5day, file.name = "CombSpillWay_fMeHgFlux_lr5_Day2013-2019.cvs")
preds_Flux_lr5WY<-predLoad(fMeHg_CombSpillWay_lr5,CombSpillWayQ,load.units="kg",by="water year",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
preds_Flux_lr5WY
exportCSV(preds_Flux_lr5WY, file.name = "CombSpillWay_fMeHg_Flux_lr5_WY2013-2019.cvs")
