#predict concntrations
###############################################################################################
preds_conc_lr5 <- predConc(fMeHg_Road102_lr5, Road102Q, by= "day", allow.incomplete= TRUE )
preds_conc_lr5
exportCSV(preds_conc_lr5, file.name = "Road102_fMeHg_conc_lr5_2010-2017.cvs")
