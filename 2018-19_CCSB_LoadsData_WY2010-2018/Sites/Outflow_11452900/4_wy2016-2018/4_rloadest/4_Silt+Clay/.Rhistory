SiltClay_Outflow_load<-predLoad(SiltClay_Outflowm3,OutflowQ,load.units="kg",by="water year",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
write.csv(SiltClay_Outflow_load,"4_Outflow_SiltClay_m3_Flux_Annual_uncorr.csv")
