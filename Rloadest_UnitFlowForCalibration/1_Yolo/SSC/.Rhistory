flow.units="cfs", conc.units="mg/L", load.units="kg",
station="Cache Creek at Yolo")
setSweave("SSC_Yolo_lr6_plot2",5,5)
plot(SSC_Yolo_lr6,which=1,set.up=FALSE) #plot 1: Calibrated regression model
graphics.off()
setSweave("SSC_Yolo_lr6_plot1",5,5)
plot(SSC_Yolo_lr6,which=2,set.up=FALSE) #plot 2:Estimates of log load versus model residuals from AMLE regression
graphics.off()
setSweave("SSC_Yolo_lr6_plot3",5,5)
plot(SSC_Yolo_lr6,which=3,set.up=FALSE)# plot 3: Estimates of log load versus squared model residuals
graphics.off()
setSweave("SSC_Yolo_lr6_plot4",5,5)
plot(SSC_Yolo_lr6,which=4,set.up=FALSE) # plot 4: Standardized Serial Correlation
graphics.off()
setSweave("SSC_Yolo_lr6_plot5",5,5)
plot(SSC_Yolo_lr6,which=5,set.up=FALSE) # plot 5: Normal probability plot for the model residuals (Quantile plot)
graphics.off()
#############################################################################################
#predict concntrations
###############################################################################################
preds_conc_lr6 <- predConc(SSC_Yolo_lr6, YoloQ, by= "day", allow.incomplete= TRUE )
preds_conc_lr6
exportCSV(preds_conc_lr6, file.name = "Yolo_SSC_conc_lr6_2017-2019.cvs")
