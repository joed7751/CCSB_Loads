pTHg_Spillway<-importRDB("pTHg_SpillwayR.txt")
SpillwayQ<-importRDB("SpillwayQR.txt")
pTHg_Spillwaym1 <- loadReg(pTHg ~model(1), data = pTHg_Spillway, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "CCSB-Yolo")
pTHg_Spillwaym1
