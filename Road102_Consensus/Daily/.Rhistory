##############################################################################################
###############################################################################################
SSC_Road102_lr7 <- loadReg(SSC ~ model(7), data=SSC_Road102,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="mg/L", load.units="kg",
station="Cache Creek at Road102")
setSweave("SSC_Road102_lr7_plot2",5,5)
plot(SSC_Road102_lr7,which=1,set.up=FALSE) #plot 1: Calibrated regression model
graphics.off()
setSweave("SSC_Road102_lr7_plot1",5,5)
plot(SSC_Road102_lr7,which=2,set.up=FALSE) #plot 2:Estimates of log load versus model residuals from AMLE regression
graphics.off()
setSweave("SSC_Road102_lr7_plot3",5,5)
plot(SSC_Road102_lr7,which=3,set.up=FALSE)# plot 3: Estimates of log load versus squared model residuals
graphics.off()
setSweave("SSC_Road102_lr7_plot4",5,5)
plot(SSC_Road102_lr7,which=4,set.up=FALSE) # plot 4: Standardized Serial Correlation
graphics.off()
setSweave("SSC_Road102_lr7_plot5",5,5)
plot(SSC_Road102_lr7,which=5,set.up=FALSE) # plot 5: Normal probability plot for the model residuals (Quantile plot)
graphics.off()
#############################################################################################
#predict concntrations
###############################################################################################
preds_conc_lr7 <- predConc(SSC_Road102_lr7, Road102Q, by= "day", allow.incomplete= TRUE )
preds_conc_lr7
exportCSV(preds_conc_lr7, file.name = "Road102_SSC_conc_lr7_2010-2017.cvs")
###############################################################################################
##We plot the estimated unite conc Vs the obsereved unite conc in xls
###############################################################################################
###############################################################################################
#load calculations using selected model
###############################################################################################
preds_Flux_lr7day<-predLoad(SSC_Road102_lr7,Road102Q,load.units="kg",by="day",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
preds_Flux_lr7day
exportCSV(preds_Flux_lr7day, file.name = "SSCFlux_lr7_Day2010-2017.cvs")
preds_Flux_lr7WY<-predLoad(SSC_Road102_lr7,Road102Q,load.units="kg",by="water year",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
preds_Flux_lr7WY
exportCSV(preds_Flux_lr7WY, file.name = "SSC_Flux_lr7_WY2010-2017.cvs")
