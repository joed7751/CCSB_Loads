View(SiltClay_Outflow)
SiltClay_Outflowm1 <- loadReg(Silt.Clay ~model(1), data = SiltClay_Outflow, flow="Flow", dates = "Dates" ,conc.units="mg/L" , station = "CCSB-Yolo")
SiltClay_Outflowm1
