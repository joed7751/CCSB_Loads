library(rloadest)
library(dataRetrieval)
library(ggplot2)
library(loadflex)
##############################################################################################
#Importing DATA for Cache Creek at GS
##############################################################################################
# Interpolation data:GS wwMeHg data grab sample observations
wwMeHg_GS <- importRDB("GatedSpillWay_wwMeHg.txt")
wwMeHg_GS
### Reading in Flow Data
GSQ <- importRDB("GatedSpillWay_DQ.txt")
GSQ$Dates <- with(GSQ, as.POSIXct(paste(Dates), format= "%Y-%m-%d"))
GSQ
app1.lr <- selBestModel("wwMeHg", data = wwMeHg_GS,
flow = "Flow", dates = "Dates", conc.units="ng/L",
station="Cache Creek at GS")
print(app1.lr)
wwMeHg_GS_lr1 <- loadReg(wwMeHg ~ model(1), data=wwMeHg_GS,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L",load.units="kg",
station="Cache Creek at GS")
wwMeHg_GS_lr1
wwMeHg_GS_lr2 <- loadReg(wwMeHg ~ model(2), data=wwMeHg_GS,
flow="Flow", date="Dates", time.step="day",
