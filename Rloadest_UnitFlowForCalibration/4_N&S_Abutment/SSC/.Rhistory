preds_conc_lr1 <- predConc(SSC_CombSpillWay_lr1, CombSpillWayQ, by= "day", allow.incomplete= TRUE )
preds_conc_lr1
exportCSV(preds_conc_lr1, file.name = "CombSpillWay_SSC_conc_lr1_2013-2019.cvs")
