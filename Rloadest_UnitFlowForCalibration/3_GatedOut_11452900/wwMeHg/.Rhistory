library(rloadest)
library(dataRetrieval)
library(ggplot2)
library(loadflex)
##############################################################################################
#Importing DATA for Cache Creek at Gated
##############################################################################################
# Interpolation data:Gated wwMeHg data grab sample observations
wwMeHg_Gated <- importRDB("GatedOut_wwMeHg.txt")
wwMeHg_Gated
### Reading in Flow Data
GatedQ <- importRDB("GatedOutflow_DQ.txt")
GatedQ$Dates <- with(GatedQ, as.POSIXct(paste(Dates), format= "%Y-%m-%d"))
GatedQ
##############################################################################################
#Runing Load Models
##############################################################################################
###Selecting best fit model###############
app1.lr <- selBestModel("wwMeHg", data = wwMeHg_Gated,
flow = "Flow", dates = "Dates", conc.units="ng/L",
station="Cache Creek at Gated")
print(app1.lr)
wwMeHg_Gated_lr5 <- loadReg(wwMeHg ~ model(5), data=wwMeHg_Gated,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L", load.units="kg",
station="Cache Creek at Gated")
setSweave("wwMeHg_Gated_lr5_plot2",5,5)
plot(wwMeHg_Gated_lr5,which=1,set.up=FALSE) #plot 1: Calibrated regression model
graphics.off()
setSweave("wwMeHg_Gated_lr5_plot1",5,5)
plot(wwMeHg_Gated_lr5,which=2,set.up=FALSE) #plot 2:Estimates of log load versus model residuals from AMLE regression
graphics.off()
setSweave("wwMeHg_Gated_lr5_plot3",5,5)
plot(wwMeHg_Gated_lr5,which=3,set.up=FALSE)# plot 3: Estimates of log load versus squared model residuals
graphics.off()
setSweave("wwMeHg_Gated_lr5_plot4",5,5)
plot(wwMeHg_Gated_lr5,which=4,set.up=FALSE) # plot 4: Standardized Serial Correlation
graphics.off()
setSweave("wwMeHg_Gated_lr5_plot5",5,5)
plot(wwMeHg_Gated_lr5,which=5,set.up=FALSE) # plot 5: Normal probability plot for the model residuals (Quantile plot)
graphics.off()
#############################################################################################
#predict concntrations
###############################################################################################
preds_conc_lr5 <- predConc(wwMeHg_Gated_lr5, GatedQ, by= "day", allow.incomplete= TRUE )
preds_conc_lr5
exportCSV(preds_conc_lr5, file.name = "Gated_wwMeHg_conc_lr5_2010-2019.cvs")
###############################################################################################
##We plot the estimated unite conc Vs the obsereved unite conc in xls
###############################################################################################
###############################################################################################
#load calculations using selected model
###############################################################################################
preds_Flux_lr5day<-predLoad(wwMeHg_Gated_lr5,GatedQ,load.units="kg",by="day",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
preds_Flux_lr5day
exportCSV(preds_Flux_lr5day, file.name = "Gated_wwMeHgFlux_lr5_Day2010-2019.cvs")
preds_Flux_lr5WY<-predLoad(wwMeHg_Gated_lr5,GatedQ,load.units="kg",by="water year",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
preds_Flux_lr5WY
exportCSV(preds_Flux_lr5WY, file.name = "Gated_wwMeHg_Flux_lr5_WY2010-2019.cvs")
