plot(SiltClay_Outflowm3,ann=FALSE)
title(main = "4_11452900_SiltClay_m3 Response vs Fitted Values",xlab = "Fitted Values",ylab = "Response Values")
plot(SiltClay_Outflowm3,which = 2,set.up = F)
