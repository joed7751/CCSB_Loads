fTHg_Inlet_load<-predLoad(fTHg_Inletm3,InletQ,load.units="kg",by="water year",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
write.csv(fTHg_Inlet_load,"1_Inlet_fTHg_m3_Flux_Annual.csv")
fTHg_Inlet_load_day<-predLoad(fTHg_Inletm3, InletQ,load.units = "kg",by="day",allow.incomplete = TRUE,conf.int = 0.90,print = TRUE)
write.csv(fTHg_Inlet_load_day,"1_Inlet_fTHg_m3_Flux_Daily.csv")
