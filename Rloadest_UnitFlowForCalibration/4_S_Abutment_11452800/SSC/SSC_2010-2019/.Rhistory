SSC_South_lr1 <- loadReg(SSC ~ model(1), data=SSC_South,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="mg/L", load.units="kg",
station="Cache Creek at South")
setSweave("SSC_South_lr1_plot2",5,5)
plot(SSC_South_lr1,which=1,set.up=FALSE) #plot 1: Calibrated regression model
graphics.off()
setSweave("SSC_South_lr1_plot1",5,5)
plot(SSC_South_lr1,which=2,set.up=FALSE) #plot 2:Estimates of log load versus model residuals from AMLE regression
graphics.off()
setSweave("SSC_South_lr1_plot3",5,5)
plot(SSC_South_lr1,which=3,set.up=FALSE)# plot 3: Estimates of log load versus squared model residuals
graphics.off()
setSweave("SSC_South_lr1_plot4",5,5)
plot(SSC_South_lr1,which=4,set.up=FALSE) # plot 4: Standardized Serial Correlation
graphics.off()
setSweave("SSC_South_lr1_plot5",5,5)
plot(SSC_South_lr1,which=5,set.up=FALSE) # plot 5: Normal probability plot for the model residuals (Quantile plot)
graphics.off()
#############################################################################################
#predict concntrations
###############################################################################################
preds_conc_lr1 <- predConc(SSC_South_lr1, SouthQ, by= "day", allow.incomplete= TRUE )
preds_conc_lr1
exportCSV(preds_conc_lr1, file.name = "South_SSC_conc_lr1_2010-2019.cvs")
###############################################################################################
##We plot the estimated unite conc Vs the obsereved unite conc in xls
###############################################################################################
###############################################################################################
#load calculations using selected model
###############################################################################################
preds_Flux_lr1day<-predLoad(SSC_South_lr1,SouthQ,load.units="kg",by="day",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
preds_Flux_lr1day
exportCSV(preds_Flux_lr1day, file.name = "South_SSCFlux_lr1_Day2010-2019.cvs")
preds_Flux_lr1WY<-predLoad(SSC_South_lr1,SouthQ,load.units="kg",by="water year",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
preds_Flux_lr1WY
exportCSV(preds_Flux_lr1WY, file.name = "South_SSC_Flux_lr1_WY2010-2019.cvs")
