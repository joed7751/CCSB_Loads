library(rloadest)
library(dataRetrieval)
library(ggplot2)
library(loadflex)
##############################################################################################
#Importing DATA for Cache Creek at GS
##############################################################################################
# Interpolation data:GS fMeHg data grab sample observations
fMeHg_GS <- importRDB("GatedSpillWay_fMeHg.txt")
fMeHg_GS
## Reading in Flow Data
GSQ <- importRDB("GatedSpillWay_DQ.txt")
GSQ$Dates <- with(GSQ, as.POSIXct(paste(Dates), format= "%Y-%m-%d"))
GSQ
##############################################################################################
#Runing Load Models
##############################################################################################
###Selecting best fit model###############
app1.lr <- selBestModel("fMeHg", data = fMeHg_GS,
flow = "Flow", dates = "Dates", conc.units="ng/L",
station="Cache Creek at GS")
print(app1.lr)
fMeHg_GS_lr1 <- loadReg(fMeHg ~ model(1), data=fMeHg_GS,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L",load.units="kg",
station="Cache Creek at GS")
fMeHg_GS_lr1
fMeHg_GS_lr2 <- loadReg(fMeHg ~ model(2), data=fMeHg_GS,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L",load.units="kg",
station="Cache Creek at GS")
fMeHg_GS_lr2
fMeHg_GS_lr3 <- loadReg(fMeHg ~ model(3), data=fMeHg_GS,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L",load.units="kg",
station="Cache Creek at GS")
fMeHg_GS_lr3
fMeHg_GS_lr4 <- loadReg(fMeHg ~ model(4), data=fMeHg_GS,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L",load.units="kg",
station="Cache Creek at GS")
fMeHg_GS_lr4
fMeHg_GS_lr5 <- loadReg(fMeHg ~ model(5), data=fMeHg_GS,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L",load.units="kg",
station="Cache Creek at GS")
fMeHg_GS_lr5
fMeHg_GS_lr6 <- loadReg(fMeHg ~ model(6), data=fMeHg_GS,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L",load.units="kg",
station="Cache Creek at GS")
fMeHg_GS_lr6
fMeHg_GS_lr7 <- loadReg(fMeHg ~ model(7), data=fMeHg_GS,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L",load.units="kg",
station="Cache Creek at GS")
fMeHg_GS_lr7
fMeHg_GS_lr8 <- loadReg(fMeHg ~ model(8), data=fMeHg_GS,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L",load.units="kg",
station="Cache Creek at GS")
fMeHg_GS_lr8
fMeHg_GS_lr9 <- loadReg(fMeHg ~ model(9), data=fMeHg_GS,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L",load.units="kg",
station="Cache Creek at GS")
fMeHg_GS_lr9
fMeHg_GS_lr5
fMeHg_GS_lr7
fMeHg_GS_lr1 <- loadReg(fMeHg ~ model(3), data=fMeHg_GS,
flow="Flow", date="Dates", time.step="day",
flow.units="cfs", conc.units="ng/L", load.units="kg",
station="Cache Creek at GS")
setSweave("fMeHg_GS_lr3_plot2",5,5)
plot(fMeHg_GS_lr3,which=1,set.up=FALSE) #plot 1: Calibrated regression model
graphics.off()
setSweave("fMeHg_GS_lr3_plot1",5,5)
plot(fMeHg_GS_lr3,which=2,set.up=FALSE) #plot 2:Estimates of log load versus model residuals from AMLE regression
graphics.off()
setSweave("fMeHg_GS_lr3_plot3",5,5)
plot(fMeHg_GS_lr3,which=3,set.up=FALSE)# plot 3: Estimates of log load versus squared model residuals
graphics.off()
setSweave("fMeHg_GS_lr3_plot4",5,5)
plot(fMeHg_GS_lr3,which=4,set.up=FALSE) # plot 4: Standardized Serial Correlation
graphics.off()
setSweave("fMeHg_GS_lr3_plot5",5,5)
plot(fMeHg_GS_lr3,which=5,set.up=FALSE) # plot 5: Normal probability plot for the model residuals (Quantile plot)
graphics.off()
#############################################################################################
#predict concntrations
###############################################################################################
preds_conc_lr3 <- predConc(fMeHg_GS_lr3, GSQ, by= "day", allow.incomplete= TRUE )
preds_conc_lr3
exportCSV(preds_conc_lr3, file.name = "GS_fMeHg_conc_lr3_2010-2019.cvs")
###############################################################################################
##We plot the estimated unite conc Vs the obsereved unite conc in xls
###############################################################################################
###############################################################################################
#load calculations will be done outside Loadest
#But this load calculated will be used to estimate SE for the xlxs calculation
###############################################################################################
preds_Flux_lr3day<-predLoad(fMeHg_GS_lr3,GSQ,load.units="kg",by="day",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
preds_Flux_lr3day
exportCSV(preds_Flux_lr3day, file.name = "GS_fMeHgFlux_lr3_Day2010-2019.cvs")
preds_Flux_lr3WY<-predLoad(fMeHg_GS_lr3,GSQ,load.units="kg",by="water year",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
preds_Flux_lr3WY
exportCSV(preds_Flux_lr3WY, file.name = "GS_fMeHgFlux_lr3_WY2010-2019.cvs")
fMeHg_GS_lr3
